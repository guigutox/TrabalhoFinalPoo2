<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.BuscaController">
   <children>
      <ToolBar prefHeight="40.0" prefWidth="640.0">
        <items>
          <Button mnemonicParsing="false" onAction="#trocarPrincipal" text="Inicio" />
            <Button mnemonicParsing="false" text="Busca" />
        </items>
      </ToolBar>
      <TextField fx:id="txt_cpf" layoutX="20.0" layoutY="93.0" />
      <Label layoutX="20.0" layoutY="63.0" prefHeight="17.0" prefWidth="214.0" text="Insira o cpf da passagem a ser buscada:" />
      <Button layoutX="20.0" layoutY="128.0" mnemonicParsing="false" onAction="#buscarPassagem" text="Buscar" />
      <TextArea fx:id="txt_resultado" layoutX="349.0" layoutY="80.0" prefHeight="203.0" prefWidth="264.0" />
      <Button layoutX="561.0" layoutY="294.0" mnemonicParsing="false" onAction="#deletarPassagem" text="Apagar" />
   </children>
</AnchorPane>
